{
    "manifest_version": 2,
    "name": "debugHunter",
    "version": "1.0.0",
    "description": "Discover hidden debugging parameters and uncover web application secrets",
    "icons": {
      "48": "images/icon.png"
    },
    "permissions": [
      "webRequest",
      "webRequestBlocking",
      "<all_urls>"
    ],
    "background": {
      "scripts": ["similarity.min.js", "background.js"],
      "persistent": true
    },
    "browser_action": {
      "default_icon": "images/icon.png",
      "default_popup": "popup.html"
    }
  }
  
