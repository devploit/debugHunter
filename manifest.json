{
  "manifest_version": 2,
  "name": "debugHunter",
  "version": "1.1.1",
  "description": "Discover hidden debugging parameters/headers and uncover web application secrets",
  "options_page": "options.html",
  "icons": {
    "48": "images/icon.png"
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "storage",
    "<all_urls>",
    "tabs"
  ],
  "background": {
    "scripts": ["similarity.min.js", "debugHunter.js"],
    "persistent": true
  },
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_popup": "popup.html"
  }
}
  
